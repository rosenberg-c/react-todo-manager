{
  "name": "@repo-pak/api-client",
  "version": "0.0.0",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "private": true,
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "generate:openapi:fe": "rm -rf src/generated && pnpm run generate:users",
    "generate:users": "swagger-typescript-api generate -p ../../services/users/api/swagger.json -o ./src/generated/users -n api.ts --axios --modular --extract-request-params",
    "clean:generated": "rm -rf ./src/generated",
    "prebuild": "pnpm run generate:openapi:fe",
    "build": "pnpm run clean:build && tsup",
    "clean:build": "rm -rf dist",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "axios": "^1.13.2"
  },
  "devDependencies": {
    "@repo-pak/config": "workspace:*",
    "@types/node": "^25.0.3",
    "swagger-typescript-api": "^13.2.16",
    "tsup": "^8.5.1",
    "typescript": "~5.9.3",
    "vitest": "^4.0.16"
  }
}